{
    "dotnet.defaultSolution": "MethodCache.sln",
    "files.exclude": {
        "**/bin": true,
        "**/obj": true,
        "**/*.user": true,
        "**/.vs": true
    },
    "search.exclude": {
        "**/bin": true,
        "**/obj": true,
        "**/node_modules": true,
        "**/.git": true
    },
    "files.watcherExclude": {
        "**/bin/**": true,
        "**/obj/**": true,
        "**/node_modules/**": true
    },
    "dotnet.completion.showCompletionItemsFromUnimportedNamespaces": true,
    "dotnet.inlayHints.enableInlayHintsForParameters": true,
    "dotnet.inlayHints.enableInlayHintsForLiteralParameters": true,
    "dotnet.inlayHints.enableInlayHintsForIndexerParameters": true,
    "dotnet.inlayHints.enableInlayHintsForObjectCreationParameters": true,
    "dotnet.inlayHints.enableInlayHintsForOtherParameters": true,
    "dotnet.inlayHints.suppressInlayHintsForParametersThatDifferOnlyBySuffix": true,
    "dotnet.inlayHints.suppressInlayHintsForParametersThatMatchMethodIntent": true,
    "dotnet.inlayHints.suppressInlayHintsForParametersThatMatchArgumentName": true,
    "csharp.semanticHighlighting.enabled": true,
    "csharp.format.enable": true,
    "omnisharp.enableEditorConfigSupport": true,
    "omnisharp.enableRoslynAnalyzers": true,
    "terminal.integrated.env.windows": {
        "METHODCACHE_DEV_MODE": "true"
    },
    "terminal.integrated.env.linux": {
        "METHODCACHE_DEV_MODE": "true"
    },
    "terminal.integrated.env.osx": {
        "METHODCACHE_DEV_MODE": "true"
    },
    "testing.automaticallyOpenPeekView": "never",
    "dotnet.unitTests.runSettingsPath": "./test.runsettings",
    "files.associations": {
        "*.md": "markdown",
        "test.runsettings": "xml",
        "appsettings*.json": "jsonc"
    },
    "markdown.preview.doubleClickToSwitchToEditor": false,
    "markdown.preview.markEditorSelection": true,
    "git.ignoreLimitWarning": true,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "*.csproj": "*.csproj.user,*.csproj.DotSettings",
        "appsettings.json": "appsettings.*.json",
        "README.md": "CHANGELOG.md,CONTRIBUTING.md,LICENSE*,*.md"
    },
    "workbench.colorCustomizations": {
        "terminal.ansiBlue": "#3794ff",
        "terminal.ansiGreen": "#3fb950",
        "terminal.ansiRed": "#f85149",
        "terminal.ansiYellow": "#d29922"
    },
    "editor.rulers": [120],
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit",
        "source.fixAll": "explicit"
    }
}