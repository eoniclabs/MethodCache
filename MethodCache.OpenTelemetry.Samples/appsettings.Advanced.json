{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "MethodCache": "Debug",
      "MethodCache.OpenTelemetry": "Information"
    }
  },
  "MethodCache": {
    "OpenTelemetry": {
      "EnableTracing": true,
      "EnableMetrics": true,
      "EnableHttpCorrelation": true,
      "EnableBaggagePropagation": true,
      "EnableDistributedTracing": true,
      "EnableStorageProviderInstrumentation": true,
      "RecordCacheKeys": true,
      "HashCacheKeys": true,
      "SamplingRatio": 1.0,
      "ExportSensitiveData": false,
      "MetricExportInterval": "00:00:30",
      "ServiceName": "MethodCacheAdvancedSample",
      "ServiceVersion": "2.0.0",
      "Environment": "Development"
    },
    "Security": {
      "EnablePIIDetection": true,
      "PIITypesToDetect": "EmailAddress,PhoneNumber,SocialSecurityNumber,CreditCardNumber",
      "DefaultRedactionStrategy": "Hash",
      "PIIConfidenceThreshold": 0.8,
      "TypeSpecificStrategies": {
        "EmailAddress": "PartialMask",
        "PhoneNumber": "Mask",
        "CreditCardNumber": "Remove"
      },
      "AlwaysRedactFields": [
        "password",
        "secret",
        "token",
        "apiKey",
        "connectionString"
      ],
      "NeverRedactFields": [
        "publicId",
        "displayName"
      ],
      "EnableAttributeEncryption": false,
      "MaxAttributeLength": 1000,
      "EnableDLPLogging": true
    },
    "Correlation": {
      "EnableDatabaseCorrelation": true,
      "EnableMessageQueueCorrelation": true,
      "EnableBackgroundJobCorrelation": true,
      "EnableDistributedLockCorrelation": true,
      "EnableExternalServiceCorrelation": true,
      "MaxActiveCorrelations": 10000,
      "CorrelationTimeout": "00:30:00",
      "EnableCleanup": true,
      "CleanupInterval": "00:05:00"
    },
    "Exporters": {
      "CustomLogging": {
        "Enabled": true,
        "MaxBatchSize": 100,
        "ExportTimeoutMs": 5000,
        "MaxRetries": 3,
        "RetryDelayMs": 1000,
        "EnableCompression": false
      }
    },
    "Defaults": {
      "Duration": "00:05:00"
    }
  },
  "OpenTelemetry": {
    "ResourceAttributes": {
      "service.name": "MethodCacheAdvancedSample",
      "service.version": "2.0.0",
      "service.environment": "development"
    },
    "Instrumentation": {
      "AspNetCore": {
        "RecordException": true,
        "EnableGrpcAspNetCoreSupport": false
      },
      "HttpClient": {
        "RecordException": true
      }
    },
    "Tracing": {
      "Sampler": {
        "Name": "TraceIdRatioBased",
        "Arguments": {
          "ratio": 1.0
        }
      }
    },
    "Metrics": {
      "PeriodicExportingMetricReaderOptions": {
        "ExportIntervalMilliseconds": 30000,
        "ExportTimeoutMilliseconds": 30000
      }
    }
  },
  "AllowedHosts": "*"
}