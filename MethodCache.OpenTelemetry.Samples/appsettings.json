{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "MethodCache": "Debug"
    }
  },
  "OpenTelemetry": {
    "Endpoint": "http://localhost:4317",
    "ServiceName": "MethodCacheSample",
    "ServiceVersion": "1.0.0"
  },
  "MethodCache": {
    "Defaults": {
      "Duration": "00:05:00"
    }
  },
  "AllowedHosts": "*"
}